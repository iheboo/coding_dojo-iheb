@{
    ViewData["FristName"] = "Home Page";
}
@model Chef
<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p><a href="Chefs">chefs</a>|<a href="">Add Chef </a>|<a href="">dishes</a>|<a href="createDishes">ADD dishes</a></p>
   <form method="post" asp-action="CreateChef" >
        <div class="form-group">
            <label asp-for="FristName"></label>
            <span asp-validation-for="FristName" class="text-danger"></span>
            <input asp-for="FristName" class="form-control">
        </div>
        <div class="form-group">
            <label asp-for="LastName"></label>
            <span asp-validation-for="LastName" class="text-danger"></span>
            <input asp-for="LastName" class="form-control">
        </div>
        <div class="form-group">
            <label asp-for="Year"></label>
            <span asp-validation-for="Year" class="text-danger"></span>
            <input asp-for="Year" class="form-control">
        </div>
        <input type="submit" value="Add Chef" class="btn btn-info">
   </form>
</div>
