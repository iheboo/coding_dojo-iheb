@model IEnumerable<Dich>
<h1>welcome to CRUDelicious</h1>
<div class="container ">
<div class="d-flex ">
<h1><a asp-action="Index">Home </a> | <a asp-action="New">Add a dish</a></h1>
</div>
<div class="d-flex  flex-column">
    <h3>Check out some recent dishes:</h3>
    @foreach (var dich in Model)
    {
       <p><a  href="@dich.DichId" >@dich.Name</a>by @dich.Chef</p> 
    }
</div>
</div>